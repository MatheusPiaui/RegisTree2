<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.registree.registree.Main3Activity">

    <EditText
        android:id="@+id/textBairro"
        android:layout_width="344dp"
        android:layout_height="45dp"
        android:layout_marginBottom="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:hint="Bairro"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/textRua"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="67dp" />

    <EditText
        android:id="@+id/textRua"
        android:layout_width="342dp"
        android:layout_height="42dp"
        android:layout_marginBottom="-1dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:hint="Rua"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/textNumero"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="-42dp"
        tools:layout_editor_absoluteY="113dp" />

    <EditText
        android:id="@+id/textNumero"
        android:layout_width="340dp"
        android:layout_height="44dp"
        android:layout_marginBottom="5dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:hint="Número"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/textReferencia"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="-40dp"
        tools:layout_editor_absoluteY="153dp" />

    <EditText
        android:id="@+id/textReferencia"
        android:layout_width="338dp"
        android:layout_height="46dp"
        android:layout_marginBottom="178dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:hint="Ponto de Referência"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/btnTela3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="-38dp"
        tools:layout_editor_absoluteY="199dp" />

    <Button
        android:id="@+id/btnTela3"
        android:layout_width="161dp"
        android:layout_height="38dp"
        android:layout_marginBottom="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:background="@color/colorAccent"
        android:onClick="chamarTela4"
        android:text="Próximo"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="433dp" />

</android.support.constraint.ConstraintLayout>
